						<li>Torso</li>
					</groups>
					<parts>
						<li>
							<def>MechanicalHead</def>
							<customLabel>head</customLabel>
							<coverage>0.72</coverage>
							<groups>
								<li>UpperHead</li>
								<li>FullHead</li>
							</groups>
							<parts>
								<li>
									<def>ArtificialBrain</def>
									<customLabel>artificial brain</customLabel>
									<coverage>0.25</coverage>
									<depth>Inside</depth>
									<groups>
										<li>UpperHead</li>
										<li>FullHead</li>
									</groups>
								</li>
								<li>
									<def>BaseRobot_UnneededData</def>
