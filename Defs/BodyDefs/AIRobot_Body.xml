									<depth>Inside</depth>
									<groups>
										<li>Torso</li>
									</groups>
								</li>
							</parts>
						</li>
					</parts>
				</li>
				<li>
					<def>MechanicalShoulder</def>
					<customLabel>left mechanical shoulder</customLabel>
					<coverage>0.1</coverage>
					<groups>
						<li>Shoulders</li>
					</groups>
					<parts>
						<li>
							<def>MechanicalArm</def>
							<customLabel>left mechanical arm</customLabel>
							<coverage>0.95</coverage>
							<groups>
								<li>Arms</li>
							</groups>
							<parts>
								<li>
									<def>MechanicalHand</def>
